<?xml version="1.0" encoding="UTF-8"?>

<PropertyList>

  <chase-distance-m type="double" archive="y"> -50 </chase-distance-m>

  <view n="100">
    <name>Co pilote</name>
    <type>lookfrom</type>
    <internal archive="y">true</internal>
    <config>
      <from-model type="bool">true</from-model>
      <from-model-idx type="int">0</from-model-idx>
      <ground-level-nearplane-m type="double">0.01f</ground-level-nearplane-m>
      <pitch-offset-deg>-5</pitch-offset-deg>
      <x-offset-m archive="y">  0.50 </x-offset-m> <!-- Right -->
      <y-offset-m archive="y">  5.75 </y-offset-m> <!-- Up    -->
      <z-offset-m archive="y"> -21.75 </z-offset-m> <!-- Back  -->
      <pitch-offset-deg> -14 </pitch-offset-deg>
      <default-field-of-view-deg> 85 </default-field-of-view-deg>
    </config>
  </view>

  <view n="101">
    <name>Tail Camera View</name>
    <type>lookfrom</type>
    <internal archive="y">true</internal>
    <config>
      <from-model type="bool">true</from-model>
      <from-model-idx type="int">0</from-model-idx>
      <ground-level-nearplane-m type="double">0.01f</ground-level-nearplane-m>
      <x-offset-m archive="y">  0.04 </x-offset-m> <!--Right-->
      <y-offset-m archive="y">  13.60 </y-offset-m> <!--Up-->
      <z-offset-m archive="y"> 20.55 </z-offset-m> <!--Back-->
      <pitch-offset-deg> -5 </pitch-offset-deg>
      <default-field-of-view-deg type="double"> 40 </default-field-of-view-deg>
    </config>
  </view>

  <view n="102">
    <name>Cabin Camera View</name>
    <type>lookfrom</type>
    <internal archive="y">true</internal>
    <config>
      <from-model type="bool">true</from-model>
      <from-model-idx type="int">0</from-model-idx>
      <ground-level-nearplane-m type="double">0.01f</ground-level-nearplane-m>
      <x-offset-m archive="y">   0.000 </x-offset-m> <!--Right-->
      <y-offset-m archive="y">  5.54 </y-offset-m> <!--Up-->
      <z-offset-m archive="y"> -20.04 </z-offset-m> <!--Back-->
      <pitch-offset-deg> -3 </pitch-offset-deg>
      <default-field-of-view-deg type="double"> 90 </default-field-of-view-deg>
    </config>
  </view>
  
  <view n="103">
    <name>Engineer View</name>
    <type>lookfrom</type>
    <internal archive="y">true</internal>
    <config>
      <from-model type="bool">true</from-model>
      <from-model-idx type="int">0</from-model-idx>
      <ground-level-nearplane-m type="double">0.01f</ground-level-nearplane-m>
      <x-offset-m archive="y">   0.79 </x-offset-m> <!--Right-->
      <y-offset-m archive="y">  5.14 </y-offset-m> <!--Up-->
      <z-offset-m archive="y"> -20.68 </z-offset-m> <!--Back-->
      <pitch-offset-deg> 0 </pitch-offset-deg>
      <default-field-of-view-deg type="double"> 85 </default-field-of-view-deg>
      <heading-offset-deg type="double">180 </heading-offset-deg>
    </config>
  </view>

  <view n="0">
    <internal archive="y">true</internal>
    <config>
      <x-offset-m archive="y">  -0.50 </x-offset-m> <!-- Right -->
      <y-offset-m archive="y">  5.65 </y-offset-m> <!-- Up    -->
      <z-offset-m archive="y"> -21.75 </z-offset-m> <!-- Back  -->
      <pitch-offset-deg> -10 </pitch-offset-deg>
      <default-field-of-view-deg> 95 </default-field-of-view-deg>
    </config>
  </view>

  <view n="1">
    <config>
      <target-z-offset-m archive="y" type="double"> 0 </target-z-offset-m>
    </config>
  </view>

  <view n="2">
    <config>
      <target-z-offset-m archive="y" type="double"> 0 </target-z-offset-m>
    </config>
  </view>

  <view n="3">
    <config>
      <target-z-offset-m archive="y" type="double"> 0 </target-z-offset-m>
    </config>
  </view>

  <view n="4">
    <config>
      <target-z-offset-m archive="y" type="double"> 0 </target-z-offset-m>
    </config>
  </view>

  <view n="5">
    <config>
      <target-z-offset-m archive="y" type="double"> 0 </target-z-offset-m>
    </config>
  </view>

  <view n="6">
    <config>
      <target-z-offset-m archive="y" type="double"> 0 </target-z-offset-m>
    </config>
  </view>

  <view n="7">
    <config>      
      <y-offset-m archive="y"> 10 </y-offset-m>
    </config>
  </view>

</PropertyList>

