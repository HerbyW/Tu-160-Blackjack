<PropertyList>
  
  <key n="113">
    <name>q</name>
    <desc>WingSweep to Nose</desc>
    <binding>
      <command>property-adjust</command>
      <property>/controls/flight/wing-sweep</property>
      <step type="double">-0.33334</step>
    </binding>
  </key>
  
  <key n="114">
    <name>r</name>
    <desc>Flares On-Off</desc>
    <binding>
      <command>property-toggle</command>
      <property>sim/multiplay/generic/int[1]</property>
    </binding>    
  </key>
  
  <key n="81">
    <name>Q</name>
    <desc>WingSweep to Tail</desc>
    <binding>
      <command>property-adjust</command>
      <property>/controls/flight/wing-sweep</property>
      <step type="double">0.33334</step>
    </binding>
  </key>
  
  <key n="100">
    <name>d</name>
    <desc>Retract Slats</desc>
    <binding>
      <command>property-adjust</command>
      <property>/controls/flight/slats</property>
      <step type="double">-0.25</step>
    </binding>
  </key>
  
  <key n="102">
    <name>f</name>
    <desc>Extend Slats</desc>
    <binding>
      <command>property-adjust</command>
      <property>/controls/flight/slats</property>
      <step type="double">0.25</step>
    </binding>
  </key>
  
  <key n="115">
    <name>s</name>
    <desc>Autostart</desc>
    <binding>
      <command>property-toggle</command>
      <property>sim/model/autostart</property><!--<property>controls/autostart</property>-->
    </binding>
  </key>
  
  <key n="60">
    <name> > </name>
    <desc>Change Pilot Engineer View</desc>
    <binding>
      <command>property-cycle</command>
      <property>sim/current-view/view-number</property>  
      <value>0</value>
      <value>11</value>
    </binding>
  </key>
  
  <key n="121">
    <name>y</name>
    <desc>Change Pilot Ramp View</desc>
    <binding>
      <command>property-cycle</command>
      <property>sim/current-view/view-number</property>  
      <value>0</value>
      <value>10</value>
    </binding>
  </key>
  
  <key n="127">
    <name>Delete</name>
    <desc>Toggle Reversers</desc>	
    <binding>
      <command>property-toggle</command>
      <property>controls/reverser</property>
    </binding>
  </key>
  
  <key n="101">
    <name>e</name>
    <desc>Front Cargo Door</desc>
    <binding>
        <command>property-toggle</command>
        <property>controls/cargodoor/signal</property>
      </binding>   
  </key>
  
  <key n="258">
    <name>F2</name>
    <desc>Paracchutes Deploying</desc>
    <binding>
      <command>property-toggle</command>
      <property>controls/parachutes/signal</property>
    </binding>
    <binding>
      <command>property-toggle</command>
      <property>sim/multiplay/generic/int[2]</property>
    </binding>
  </key>  

  </PropertyList>